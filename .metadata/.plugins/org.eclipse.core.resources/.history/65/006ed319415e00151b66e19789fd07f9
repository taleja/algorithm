package array;

public class ArrayUtil {

	public static int[] subArray(int[] array, int k, int j) {
		int[] rv = new int[array.length - j + k];
		
		for(int i = 0; i < array.length; i++){
			if(i >= k && i=j){
				rv[i] = array[i];
				System.out.println("array lenght: " + rv.length);
				System.out.println(array[i] + "/n");
			}
		}		
		return rv;
	}
}
