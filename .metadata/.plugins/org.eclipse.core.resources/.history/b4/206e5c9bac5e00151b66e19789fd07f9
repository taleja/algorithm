package array;

public class ArrayUtil {

	public static int[] subArray(int[] array, int fromIndex, int j) {
		int[] rv = new int[(j - fromIndex) + 1];
		//System.out.println("rv lenght: " + rv.length);

		for(int i = 0; i < array.length; i++){
			if(fromIndex <= j){
				rv[i] = array[fromIndex];
				fromIndex++;				
			}
		}		
		return rv;
	}
}
